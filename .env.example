# Google Drive Configuration
# Folder ID from your share link (the part after /folders/ in the URL)
GOOGLE_DRIVE_FOLDER_ID=
# API key from Google Cloud Console (enable Drive API first)
GOOGLE_API_KEY=

# Publish Page Protection
PUBLISH_PASSWORD=

# Netlify Build Hook
# Create at: Netlify Dashboard > Site > Site configuration > Build & deploy > Build hooks
NETLIFY_BUILD_HOOK_URL=

# Netlify API (for deploy status)
# Site ID: Netlify Dashboard > Site configuration > General > Site ID
NETLIFY_SITE_ID=
# API Token: Netlify User settings > Applications > Personal access tokens
NETLIFY_API_TOKEN=
